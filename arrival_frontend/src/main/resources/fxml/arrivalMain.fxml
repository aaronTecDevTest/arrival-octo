<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<VBox xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8"
      fx:controller="com.arrival.windows.controller.ControllerArrivalMainApp">
    <children>
        <MenuBar VBox.vgrow="ALWAYS">
            <menus>
                <Menu mnemonicParsing="false" text="\%menu.title.file">
                    <items>
                        <MenuItem mnemonicParsing="false" text="\%menu.title.new"/>
                        <MenuItem mnemonicParsing="false" text="\%menu.title.open"/>
                        <Menu mnemonicParsing="false" text="\%menu.title.open.recent"/>
                        <SeparatorMenuItem mnemonicParsing="false"/>
                        <MenuItem mnemonicParsing="false" text="\%menu.title.close"/>
                        <MenuItem mnemonicParsing="false" text="\%menu.title.save"/>
                        <MenuItem mnemonicParsing="false" text="\%menu.title.save.as"/>
                        <MenuItem mnemonicParsing="false" text="\%menu.title.revert"/>
                        <SeparatorMenuItem mnemonicParsing="false"/>
                        <MenuItem mnemonicParsing="false" text="\%menu.title.preferences"/>
                        <SeparatorMenuItem mnemonicParsing="false"/>
                        <MenuItem mnemonicParsing="false" text="\%menu.title.quit"/>
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="\%menu.title.edit">
                    <items>
                        <MenuItem mnemonicParsing="false" text="\%menu.title.undo"/>
                        <MenuItem mnemonicParsing="false" text="\%menu.title.redo"/>
                        <SeparatorMenuItem mnemonicParsing="false"/>
                        <MenuItem mnemonicParsing="false" text="\%menu.title.cut"/>
                        <MenuItem mnemonicParsing="false" text="\%menu.title.copy"/>
                        <MenuItem mnemonicParsing="false" text="\%menu.title.paste"/>
                        <MenuItem mnemonicParsing="false" text="\%menu.title.delete"/>
                        <SeparatorMenuItem mnemonicParsing="false"/>
                        <MenuItem mnemonicParsing="false" text="\%menu.title.select.all"/>
                        <MenuItem mnemonicParsing="false" text="\%menu.title.unselect.all"/>
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="\%menu.title.help">
                    <items>
                        <MenuItem mnemonicParsing="false" text="\%menu.title.about"/>
                    </items>
                </Menu>
            </menus>
        </MenuBar>
        <BorderPane VBox.vgrow="ALWAYS">
            <center>
                <SplitPane dividerPositions="0.5" BorderPane.alignment="CENTER">
                    <items>
                        <VBox maxWidth="300.0">
                            <children>
                                <HBox>
                                    <children>
                                        <Label fx:id="lblSearchfield" minWidth="48.0" prefHeight="25.0"
                                               prefWidth="100.0" text="\%label.search">
                                            <padding>
                                                <Insets left="5.0"/>
                                            </padding>
                                        </Label>
                                        <TextField fx:id="txtSearchfield" prefWidth="230.0" promptText="Testcasename"/>
                                    </children>
                                    <padding>
                                        <Insets bottom="2.0" top="2.0"/>
                                    </padding>
                                </HBox>
                                <Accordion fx:id="accTestCase" VBox.vgrow="ALWAYS">
                                    <panes>
                                        <TitledPane text="iOS - Testcase">
                                            <content>
                                                <TableView fx:id="tbvIOS" prefHeight="200.0" prefWidth="200.0"
                                                           styleClass="column-header-backgrounds">
                                                    <columns>
                                                        <TableColumn fx:id="tbcIOS" prefWidth="94.0"/>
                                                    </columns>
                                                    <columnResizePolicy>
                                                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                                                    </columnResizePolicy>
                                                </TableView>
                                            </content>
                                        </TitledPane>
                                        <TitledPane styleClass="testCase.tableView" text="Android - Testcase">
                                            <content>
                                                <TableView fx:id="tbvAND" prefHeight="200.0" prefWidth="200.0"
                                                           styleClass="column-header-backgrounds">
                                                    <columns>
                                                        <TableColumn fx:id="tbcAndroid" prefWidth="75.0"/>
                                                    </columns>
                                                    <columnResizePolicy>
                                                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                                                    </columnResizePolicy>
                                                </TableView>
                                            </content>
                                        </TitledPane>
                                        <TitledPane styleClass="testCase.tableView" text="Webportal - Testcase">
                                            <content>
                                                <TableView fx:id="tbvWebportal" prefHeight="200.0" prefWidth="200.0"
                                                           styleClass="column-header-backgrounds">
                                                    <columns>
                                                        <TableColumn fx:id="tbcWebportal" prefWidth="75.0"/>
                                                    </columns>
                                                    <columnResizePolicy>
                                                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                                                    </columnResizePolicy>
                                                </TableView>
                                            </content>
                                        </TitledPane>
                                    </panes>
                                </Accordion>
                            </children>
                        </VBox>
                        <TabPane fx:id="tbMainTabPane" tabClosingPolicy="UNAVAILABLE">
                            <tabs>
                                <Tab text="Testsuite - Regressionstest">
                                    <content>
                                        <TableView fx:id="tbvTestsuite" tableMenuButtonVisible="true">
                                            <columns>
                                                <TableColumn fx:id="tbcName" prefWidth="215.0"
                                                             text="\%tableColumn.name"/>
                                                <TableColumn fx:id="tbcDescription" prefWidth="209.0"
                                                             text="\%tableColumn.description"/>
                                                <TableColumn fx:id="tbcDuration" prefWidth="159.0"
                                                             text="\%tableColumn.duration"/>
                                                <TableColumn fx:id="tbcLastRun" prefWidth="141.0"
                                                             text="\%tableColumn.lastRun"/>
                                                <TableColumn fx:id="tbcLink" prefWidth="117.0"
                                                             text="\%tableColumn.link"/>
                                                <TableColumn fx:id="tbcResult" prefWidth="150.0"
                                                             text="\%tableColumn.result"/>
                                            </columns>
                                            <columnResizePolicy>
                                                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                                            </columnResizePolicy>
                                        </TableView>
                                    </content>
                                </Tab>
                            </tabs>
                        </TabPane>
                    </items>
                    <BorderPane.margin>
                        <Insets/>
                    </BorderPane.margin>
                </SplitPane>
            </center>
            <top>
                <ToolBar>
                    <items>
                        <Button fx:id="btnOpenTestsuite" maxHeight="34.0" maxWidth="34.0" minHeight="34.0"
                                minWidth="34.0" mnemonicParsing="false" onAction="#openTestsuite" prefHeight="34.0"
                                prefWidth="34.0">
                            <opaqueInsets>
                                <Insets/>
                            </opaqueInsets>
                            <padding>
                                <Insets bottom="2.0" left="2.0" right="2.0" top="2.0"/>
                            </padding>
                            <tooltip>
                                <Tooltip text="\%toolbar.openTestsuite"/>
                            </tooltip>
                            <graphic>
                                <ImageView fitHeight="32.0" fitWidth="32.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../icons/openFile.png"/>
                                    </image>
                                </ImageView>
                            </graphic>
                        </Button>
                        <Button fx:id="btnSaveTestsuite" maxHeight="34.0" maxWidth="34.0" minHeight="34.0"
                                minWidth="34.0" mnemonicParsing="false" onAction="#saveTestsuite" prefHeight="34.0"
                                prefWidth="34.0">

                            <padding>
                                <Insets bottom="2.0" left="2.0" right="2.0" top="2.0"/>
                            </padding>
                            <tooltip>
                                <Tooltip text="\%toolbar.saveTestsuite"/>
                            </tooltip>
                            <graphic>
                                <ImageView fitHeight="32.0" fitWidth="32.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../icons/save.png"/>
                                    </image>
                                </ImageView>
                            </graphic>
                        </Button>
                        <Button fx:id="btnCreateTestsuite" maxHeight="34.0" maxWidth="34.0" minHeight="34.0"
                                minWidth="34.0" mnemonicParsing="false" onAction="#createNewTestsuite" prefHeight="34.0"
                                prefWidth="34.0">
                            <padding>
                                <Insets bottom="2.0" left="2.0" right="2.0" top="2.0"/>
                            </padding>
                            <tooltip>
                                <Tooltip text="\%toolbar.createTestsuite"/>
                            </tooltip>
                            <graphic>
                                <ImageView fitHeight="32.0" fitWidth="32.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../icons/newFile.png"/>
                                    </image>
                                </ImageView>
                            </graphic>
                        </Button>
                        <Button fx:id="btnDeletedTestsuite" maxHeight="34.0" maxWidth="34.0" minHeight="34.0"
                                minWidth="34.0" mnemonicParsing="false" onAction="#deletedTestsuite" prefHeight="34.0"
                                prefWidth="34.0">
                            <padding>
                                <Insets bottom="2.0" left="2.0" right="2.0" top="2.0"/>
                            </padding>
                            <tooltip>
                                <Tooltip text="\%toolbar.deletedTestsuite"/>
                            </tooltip>
                            <graphic>
                                <ImageView fitHeight="32.0" fitWidth="32.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../icons/deleteFile.png"/>
                                    </image>
                                </ImageView>
                            </graphic>
                        </Button>
                        <Separator orientation="VERTICAL">
                            <padding>
                                <Insets left="5.0" right="5.0"/>
                            </padding>
                        </Separator>
                        <Button fx:id="btnAddTestcase" maxHeight="34.0" maxWidth="34.0" minHeight="34.0" minWidth="34.0"
                                mnemonicParsing="false" onAction="#addTestcaseInTestsuite" prefHeight="34.0"
                                prefWidth="34.0">
                            <padding>
                                <Insets bottom="2.0" left="2.0" right="2.0" top="2.0"/>
                            </padding>
                            <tooltip>
                                <Tooltip text="\%toolbar.addTestcase"/>
                            </tooltip>
                            <graphic>
                                <ImageView fitHeight="32.0" fitWidth="32.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../icons/add.png"/>
                                    </image>
                                </ImageView>
                            </graphic>
                        </Button>
                        <Button fx:id="btnDeletedTestcase" maxHeight="34.0" maxWidth="34.0" minHeight="34.0"
                                minWidth="34.0" mnemonicParsing="false" onAction="#deleteTestcaseFromTestsuite"
                                prefHeight="34.0" prefWidth="34.0">
                            <padding>
                                <Insets bottom="2.0" left="2.0" right="2.0" top="2.0"/>
                            </padding>
                            <tooltip>
                                <Tooltip text="\%toolbar.deletedTestcase"/>
                            </tooltip>
                            <graphic>
                                <ImageView fitHeight="32.0" fitWidth="32.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../icons/delete.png"/>
                                    </image>
                                </ImageView>
                            </graphic>
                        </Button>
                        <Separator orientation="VERTICAL">
                            <padding>
                                <Insets left="5.0" right="5.0"/>
                            </padding>
                        </Separator>
                        <Button fx:id="btnRun" maxHeight="34.0" maxWidth="34.0" minHeight="34.0" minWidth="34.0"
                                mnemonicParsing="false" onAction="#runTestsuite" prefHeight="34.0" prefWidth="34.0">
                            <padding>
                                <Insets bottom="2.0" left="2.0" right="2.0" top="2.0"/>
                            </padding>
                            <tooltip>
                                <Tooltip text="\%toolbar.run"/>
                            </tooltip>
                            <graphic>
                                <ImageView fitHeight="32.0" fitWidth="32.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../icons/play.png"/>
                                    </image>
                                </ImageView>
                            </graphic>
                        </Button>
                        <Button fx:id="btnSkip" maxHeight="34.0" maxWidth="34.0" minHeight="34.0" minWidth="34.0"
                                mnemonicParsing="false" onAction="#skipTestcase" prefHeight="34.0" prefWidth="34.0">
                            <padding>
                                <Insets bottom="2.0" left="2.0" right="2.0" top="2.0"/>
                            </padding>
                            <tooltip>
                                <Tooltip text="\%toolbar.skip"/>
                            </tooltip>
                            <graphic>
                                <ImageView fitHeight="32.0" fitWidth="32.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../icons/stepForward.png"/>
                                    </image>
                                </ImageView>
                            </graphic>
                        </Button>
                        <Button fx:id="btnPause" maxHeight="34.0" maxWidth="34.0" minHeight="34.0" minWidth="34.0"
                                mnemonicParsing="false" onAction="#pauseTestcase" prefHeight="34.0" prefWidth="34.0">
                            <padding>
                                <Insets bottom="2.0" left="2.0" right="2.0" top="2.0"/>
                            </padding>
                            <tooltip>
                                <Tooltip text="\%toolbar.pause"/>
                            </tooltip>
                            <graphic>
                                <ImageView fitHeight="32.0" fitWidth="32.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../icons/pauseNormal.png"/>
                                    </image>
                                </ImageView>
                            </graphic>
                        </Button>
                        <Button fx:id="btnStop" maxHeight="34.0" maxWidth="34.0" minHeight="34.0" minWidth="34.0"
                                mnemonicParsing="false" onAction="#stopTestsuite" prefHeight="34.0" prefWidth="34.0">
                            <tooltip>
                                <Tooltip text="\%toolbar.stop"/>
                            </tooltip>
                            <graphic>
                                <ImageView fitHeight="32.0" fitWidth="32.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../icons/stop.png"/>
                                    </image>
                                </ImageView>
                            </graphic>
                        </Button>
                        <Separator orientation="VERTICAL" prefHeight="0.0" prefWidth="0.0">
                            <padding>
                                <Insets left="5.0" right="5.0"/>
                            </padding>
                        </Separator>
                        <Button fx:id="btnResult" maxHeight="34.0" maxWidth="34.0" minHeight="34.0" minWidth="34.0"
                                mnemonicParsing="false" onAction="#showTestsuitResult" prefHeight="34.0"
                                prefWidth="34.0">
                            <tooltip>
                                <Tooltip text="\%toolbar.viewResult"/>
                            </tooltip>
                            <graphic>
                                <ImageView fitHeight="32.0" fitWidth="32.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../icons/windowAppResults.png"/>
                                    </image>
                                </ImageView>
                            </graphic>
                        </Button>
                        <Separator orientation="VERTICAL" prefHeight="0.0" prefWidth="0.0">
                            <padding>
                                <Insets left="5.0" right="5.0"/>
                            </padding>
                        </Separator>
                        <Button fx:id="btnOptions" defaultButton="true" maxHeight="34.0" maxWidth="34.0"
                                minHeight="34.0" minWidth="34.0" mnemonicParsing="false" onAction="#showOptions"
                                prefHeight="34.0" prefWidth="34.0">
                            <padding>
                                <Insets bottom="2.0" left="2.0" right="2.0" top="2.0"/>
                            </padding>
                            <tooltip>
                                <Tooltip text="\%toolbar.options"/>
                            </tooltip>
                            <graphic>
                                <ImageView fitHeight="32.0" fitWidth="32.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../icons/options.png"/>
                                    </image>
                                </ImageView>
                            </graphic>
                        </Button>
                        <Separator orientation="VERTICAL">
                            <padding>
                                <Insets left="5.0"/>
                            </padding>
                        </Separator>
                        <Pane/>
                        <Button fx:id="btnHelp" maxHeight="34.0" maxWidth="34.0" minHeight="34.0" minWidth="34.0"
                                mnemonicParsing="false" onAction="#showhelp" prefHeight="34.0" prefWidth="34.0"
                                text="Button" textAlignment="CENTER">
                            <padding>
                                <Insets bottom="1.0" left="1.0" right="1.0" top="1.0"/>
                            </padding>
                            <tooltip>
                                <Tooltip text="\%toolbar.info"/>
                            </tooltip>
                            <graphic>
                                <ImageView fitHeight="32.0" fitWidth="32.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../icons/help.png"/>
                                    </image>
                                </ImageView>
                            </graphic>
                        </Button>
                    </items>
                </ToolBar>
            </top>
        </BorderPane>
        <HBox fillHeight="false" maxHeight="30.0" prefHeight="30.0" VBox.vgrow="ALWAYS">
            <children>
                <Label fx:id="lblStatusLeft" prefHeight="30.0" text="\%label.statusbar.left" HBox.hgrow="ALWAYS">
                    <padding>
                        <Insets left="5.0" right="5.0"/>
                    </padding>
                </Label>
                <Pane prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS"/>
                <ProgressBar disable="true" prefHeight="30.0" prefWidth="300.0" progress="0.0">
                    <padding>
                        <Insets left="5.0" right="5.0"/>
                    </padding>
                    <tooltip>
                        <Tooltip text="\%statusbar.progressbar"/>
                    </tooltip>
                </ProgressBar>
                <Label fx:id="lblStatusRight" prefHeight="30.0" text="\%label.statusbar.right" HBox.hgrow="ALWAYS">
                    <padding>
                        <Insets left="5.0" right="5.0"/>
                    </padding>
                </Label>
            </children>
        </HBox>
    </children>
</VBox>
