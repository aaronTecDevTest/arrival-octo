<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>

<TabPane fx:id="tbMainTabPane" tabClosingPolicy="UNAVAILABLE" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <tabs>
      <Tab text="Testsuite - Regressionstest">
         <content>
            <TableView fx:id="tbvTestsuite" tableMenuButtonVisible="true">
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
               <columns>
                  <TableColumn fx:id="tbcName" prefWidth="215.0" text="\%tableColumn.name" />
                  <TableColumn fx:id="tbcDescription" prefWidth="209.0" text="\%tableColumn.description" />
                  <TableColumn fx:id="tbcDuration" prefWidth="159.0" text="\%tableColumn.duration" />
                  <TableColumn fx:id="tbcLastRun" prefWidth="141.0" text="\%tableColumn.lastRun" />
                  <TableColumn fx:id="tbcLink" prefWidth="117.0" text="\%tableColumn.link" />
                  <TableColumn fx:id="tbcResult" prefWidth="150.0" text="\%tableColumn.result" />
               </columns>
            </TableView>
         </content>
      </Tab>
   </tabs>
</TabPane>
